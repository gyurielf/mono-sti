{
	"name": "sti",
	"description": "sti",
	"version": "3.0.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gyurielf/mono-sti.git"
	},
	"homepage": "https://github.com/gyurielf/mono-sti#readme",
	"keywords": [
		"svelte",
		"tel input",
		"phone",
		"phone input",
		"intl",
		"intl tel input"
	],
	"engines": {
		"npm": ">= 8",
		"yarn": ">=2",
		"node": ">= 16",
		"pnpm": ">= 7"
	},
	"scripts": {
		"build": "vite build",
		"check:publint": "publint ./dist --strict",
		"dev": "vite dev",
		"eslint:fix": "eslint --fix",
		"eslint": "eslint --ext .js,.ts,.svelte .",
		"lint:fix": "npm run eslint:fix && npm run prettier:fix",
		"lint": "npm run prettier:check && npm run eslint && npm run ts && npm run svelte-check",
		"package:watch": "svelte-kit sync && svelte-package --watch",
		"package": "svelte-kit sync && svelte-package && npm run check:publint",
		"prepare:husky": "husky install",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prettier:fix": "prettier --write --plugin-search-dir=. .",
		"prettier": "prettier",
		"preview": "vite preview",
		"svelte-check": "svelte-kit sync && svelte-check --ignore 'dist,build,coverage,.svelte-kit,package' --fail-on-warnings",
		"sync": "svelte-kit sync",
		"test": "vitest",
		"ts": "tsc --noEmit"
	},
	"peerDependencies": {
		"libphonenumber-js": "^1.10.36",
		"svelte": "^3.58.0"
	},
	"dependencies": {
		"libphonenumber-js": "^1.10.36",
		"svelte": "^3.58.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.2",
		"@testing-library/svelte": "^3.2.2",
		"@testing-library/user-event": "^14.4.3",
		"@types/micromatch": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.1",
		"dotenv": "^16.3.0",
		"edit-package-json": "^0.8.14",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^22.1.0",
		"micromatch": "^4.0.5",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.12",
		"svelte-check": "^3.4.3",
		"svelte2tsx": "^0.6.15",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"license": "MIT",
	"publishConfig": {
		"directory": "dist",
		"linkDirectory": true
	}
}
