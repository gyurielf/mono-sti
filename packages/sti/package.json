{
	"name": "sti",
	"description": "sti",
	"version": "3.0.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gyurielf/mono-sti.git"
	},
	"homepage": "https://github.com/gyurielf/mono-sti#readme",
	"keywords": [
		"svelte",
		"tel input",
		"phone",
		"phone input",
		"intl",
		"intl tel input"
	],
	"engines": {
		"npm": ">= 8",
		"yarn": ">=2",
		"node": ">= 16",
		"pnpm": ">= 7"
	},
	"scripts": {
		"build": "vite build",
		"dev": "vite dev",
		"eslint": "eslint --ext .js,.ts,.svelte .",
		"eslint:fix": "eslint --fix",
		"lint": "npm run prettier:check && npm run eslint && npm run ts && npm run svelte-check",
		"lint:fix": "npm run eslint:fix && npm run prettier:fix",
		"package2": "node ./zarf/scripts/pre-build.js && svelte-kit sync && svelte-package && node ./zarf/scripts/post-build.js",
		"package:watch": "svelte-kit sync && svelte-package --watch",
		"package": "svelte-kit sync && svelte-package",
		"prettier": "prettier",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prettier:fix": "prettier --write --plugin-search-dir=. .",
		"prepare:husky": "husky install",
		"preview": "vite preview",
		"svelte-check": "svelte-kit sync && svelte-check --ignore 'dist,build,coverage,.svelte-kit,package' --fail-on-warnings",
		"test": "playwright test",
		"test:unit": "vitest",
		"ts": "tsc --noEmit",
		"sync": "svelte-kit sync"
	},
	"dependencies": {
		"libphonenumber-js": "^1.10.36",
		"svelte": "^3.58.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.2",
		"@testing-library/svelte": "^3.2.2",
		"@testing-library/user-event": "^14.4.3",
		"@types/micromatch": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.1",
		"dotenv": "^16.3.0",
		"edit-package-json": "^0.8.14",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^22.1.0",
		"micromatch": "^4.0.5",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte-check": "^3.4.3",
		"svelte2tsx": "^0.6.15",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"license": "MIT",
	"main": "./dist/index.js",
	"exports": {
		"./styles/flags.css": "./dist/styles/flags.css",
		"./types": {
			"types": "./dist/types/index.d.ts"
		},
		"./assets": {
			"types": "./dist/assets/index.d.ts",
			"default": "./dist/assets/index.js",
			"import": "./dist/assets/index.js"
		},
		"./utils": {
			"require": "./dist/utils/index.js",
			"types": "./dist/utils/index.d.ts",
			"default": "./dist/utils/index.js",
			"import": "./dist/utils/index.js"
		},
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	]
}
