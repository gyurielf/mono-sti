{
	"name": "sti",
	"description": "sti",
	"version": "3.0.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gyurielf/mono-sti.git"
	},
	"homepage": "https://github.com/gyurielf/mono-sti#readme",
	"keywords": [
		"svelte",
		"tel input",
		"phone",
		"phone input",
		"intl",
		"intl tel input"
	],
	"engines": {
		"npm": ">= 8",
		"yarn": ">=2",
		"node": ">= 16",
		"pnpm": ">= 7"
	},
	"scripts": {
		"build": "vite build",
		"check:publint": "publint --strict",
		"dev": "vite dev",
		"eslint:fix": "eslint --fix",
		"eslint": "eslint --ext .js,.ts,.svelte .",
		"lint:fix": "npm run eslint:fix && npm run prettier:fix",
		"lint": "npm run prettier:check && npm run eslint && npm run ts && npm run svelte-check",
		"package:watch": "svelte-kit sync && svelte-package --watch",
		"package": "svelte-kit sync && svelte-package && npm run check:publint",
		"prepare:husky": "husky install",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prettier:fix": "prettier --write --plugin-search-dir=. .",
		"prettier": "prettier",
		"preview": "vite preview",
		"svelte-check": "svelte-kit sync && svelte-check --ignore 'dist,build,coverage,.svelte-kit,package' --fail-on-warnings",
		"sync": "svelte-kit sync",
		"test": "vitest",
		"ts": "tsc --noEmit",
		"types": "node ./scripts/generate-dts.js"
	},
	"peerDependencies": {
		"libphonenumber-js": "^1.10.36",
		"svelte": "^3.58.0"
	},
	"dependencies": {
		"libphonenumber-js": "^1.10.36",
		"svelte": "^3.58.0"
	},
	"type": "module",
	"license": "MIT",
	"main": "./index.js",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"svelte": "./index.js",
			"default": "./index.js"
		},
		"./types": {
			"types": "./types/index.d.ts",
			"default": "./types/index.d.ts"
		},
		"./utils": {
			"types": "./utils/index.d.ts",
			"import": "./utils/index.js",
			"default": "./utils/index.js"
		},
		"./assets": {
			"types": "./assets/index.d.ts",
			"import": "./assets/index.js",
			"default": "./assets/index.js"
		},
		"./styles/flags.css": "./styles/flags.css"
	}
}
